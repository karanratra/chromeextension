function doMagic()
{
    alert('extension button clicked!!');
}

chrome.extension.onClicked.addListener(doMagic);

chrome.tabs.getSelected(null, function(tab) {
  // When the browser action is clicked, set the popup for |tab|.
  chrome.browserAction.onClicked.addListener(function(tab) {
    chrome.browserAction.setPopup({
      tabId: tab.id,          // Set the new popup for this tab.
      popup: 'karan_pop_up.html'   // Open this html file within the popup.
    });
  });
});


